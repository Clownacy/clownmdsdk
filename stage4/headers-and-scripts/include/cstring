#ifndef CSTRING_H
#define CSTRING_H

#include <cstddef>

namespace std
{
	extern "C" void* memcpy(void *dest, const void *src, std::size_t count);
	extern "C" void* memmove(void *dest, const void *src, std::size_t count);
	extern "C" void* memset(void *dest, int ch, std::size_t count);
}

#endif /* CSTRING_H */
